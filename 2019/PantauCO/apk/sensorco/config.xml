<?xml version='1.0' encoding='utf-8'?>
<widget id="id.plnm.sensorco" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CO Monitoring</name>
    <description>
        Aplikasi monitoring kadar CO ruangan menggunakan MQ-7 dengan integrasi firebase.
    </description>
    <author email="sensorco19@gmail.com" href="http://sensorco-mq7.firebaseapp.com">
        SensorCO Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
