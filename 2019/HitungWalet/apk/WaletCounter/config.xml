<?xml version='1.0' encoding='utf-8'?>
<widget id="id.plnm.waletmonitoring" version="0.1.0-alpha" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Walet Monitoring</name>
    <description>
        Aplikasi monitoring kondisi kelembapan, temperatur sarang burung walet dan jumlah total burung walet.
    </description>
    <author email="fbasewalet19@gmail.com" href="fbasewalet.firebaseapp.com">
        Fbasewalet Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
